import java.util.*;
public class main{
public static void main(String[] args){
Scanner sc = new Scanner(System.out);
int x = sc.nextInt();
for(int i=1;i<=10;i++){
System.out.println(x+" * "+i+"  = "+x*i);
}}}

/** example execution
input : 7
output : 
7 * 1  = 7
7 * 2  = 14
7 * 3  = 21
7 * 4  = 28
7 * 5  = 35
7 * 6  = 42
7 * 7  = 49
7 * 8  = 56
7 * 9  = 63
7 * 10 = 70
