import java.util.*;
public class main{
  public static void main(String[] args){
    Scanner sc = new Scanner(System.in);
    int x = sc.nextInt();
    int rev = 0;
    while(x>0){
      int y= x%10;
      rev = rev*10+y;
      x= x/10;}
    System.out.println(rev);}}

/** example execution
input = 1234
output = 4321 **/
